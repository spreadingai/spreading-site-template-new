{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false, // It was supposed to be open, so we could adapt the old version
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "/*": ["./docs/*"],
      "@site/*": ["./docs/*"]
    }
  },
  "exclude": ["node_modules", "packages"],
  "include": [
    "globals.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "docs/docs/01-Overview/test5.mdx"
  ]
}
